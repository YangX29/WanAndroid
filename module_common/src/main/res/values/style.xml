<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  进度条  -->
    <declare-styleable name="SimpleProgressBar">
        <attr name="max" format="integer" />
        <attr name="progress" format="integer" />
        <attr name="fgColor" format="color" />
        <attr name="bgColor" format="color" />
    </declare-styleable>
    <!--  开关按钮  -->
    <declare-styleable name="SwitchButton">
        <attr name="on_background" format="color|reference" />
        <attr name="off_background" format="color|reference" />
        <attr name="thumb_background" format="color|reference" />
        <attr name="thumb_padding" format="dimension" />
    </declare-styleable>
</resources>